{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["deno.window"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./functions/**/*.ts"
  ]
}
